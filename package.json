{
  "name": "intime-v3",
  "version": "3.0.0",
  "description": "InTime - Multi-Agent Staffing Platform",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "res3000": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && NODE_TLS_REJECT_UNAUTHORIZED=0 pnpm dev",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint .",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:auth": "tsx scripts/test-auth.ts",
    "test:signup": "tsx scripts/quick-signup-test.ts",
    "orchestrate": "tsx .claude/orchestration/cli/index.ts",
    "orchestrate:feature": "tsx .claude/orchestration/cli/index.ts feature",
    "orchestrate:bug": "tsx .claude/orchestration/cli/index.ts bug-fix",
    "orchestrate:review": "tsx .claude/orchestration/cli/index.ts ceo-review",
    "orchestrate:database": "tsx .claude/orchestration/cli/index.ts database",
    "orchestrate:test": "tsx .claude/orchestration/cli/index.ts test",
    "orchestrate:deploy": "tsx .claude/orchestration/cli/index.ts deploy",
    "orchestrate:artifacts": "tsx .claude/orchestration/cli/index.ts artifacts",
    "orchestrate:clear": "tsx .claude/orchestration/cli/index.ts clear",
    "timeline": "tsx tools/timeline-cli.ts",
    "timeline:add": "tsx tools/timeline-cli.ts add",
    "timeline:list": "tsx tools/timeline-cli.ts list",
    "timeline:search": "tsx tools/timeline-cli.ts search",
    "timeline:stats": "tsx tools/timeline-cli.ts stats",
    "prompt": "tsx tools/generate-prompt.ts",
    "prompt:list": "tsx tools/generate-prompt.ts list",
    "prompt:epic": "tsx tools/generate-prompt.ts generate",
    "prompt:story": "tsx tools/generate-prompt.ts generate",
    "doc:update": "tsx scripts/update-documentation.ts",
    "doc:verify": "tsx scripts/update-documentation.ts --verify-only",
    "doc:clean": "tsx scripts/update-documentation.ts --clean-only",
    "db:migrate": "tsx scripts/db-migrate.ts",
    "db:migrate:local": "tsx scripts/db-migrate.ts --local",
    "db:status": "tsx scripts/db-migrate.ts --status",
    "db:rollback": "tsx scripts/db-migrate.ts --rollback",
    "workflow": "tsx scripts/workflow-runner.ts",
    "workflow:start": "tsx scripts/workflow-runner.ts start",
    "workflow:feature": "tsx scripts/workflow-runner.ts feature",
    "workflow:epic": "tsx scripts/workflow-runner.ts epic",
    "workflow:sprint": "tsx scripts/workflow-runner.ts sprint",
    "workflow:database": "tsx scripts/workflow-runner.ts database",
    "workflow:test": "tsx scripts/workflow-runner.ts test",
    "workflow:deploy": "tsx scripts/workflow-runner.ts deploy",
    "workflow:status": "tsx scripts/workflow-runner.ts status",
    "workflow:list": "tsx scripts/workflow-runner.ts list",
    "workflow:history": "tsx scripts/workflow-runner.ts history",
    "seed:reset": "tsx scripts/seed-via-edge-function.ts",
    "seed:auth": "tsx scripts/create-auth-users.ts",
    "seed:all": "npm run seed:reset && npm run seed:auth",
    "claude:coding": "cp .claude/mcp-presets/coding.json .claude/settings.json && cp .claude/mcp-presets/coding.json .mcp.json && echo '✓ CODING mode: Supabase + Postgres + Filesystem + Sequential-Thinking. Restart Claude Code.'",
    "claude:testing": "cp .claude/mcp-presets/testing.json .claude/settings.json && cp .claude/mcp-presets/testing.json .mcp.json && echo '✓ TESTING mode: Supabase + Playwright + Sequential-Thinking. Restart Claude Code.'",
    "claude:full": "cp .claude/mcp-presets/full.json .claude/settings.json && cp .claude/mcp-presets/full.json .mcp.json && echo '✓ FULL mode: All MCPs (GitHub + Filesystem + Postgres + Playwright + Slack + Sequential-Thinking + Supabase). Restart Claude Code.'"
  },
